<DialogPane prefHeight="279.0" prefWidth="374.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sceneBuilder.UpdateInvoiceController">
   <content>
      <AnchorPane prefHeight="164.0" prefWidth="449.0" styleClass="white-background" stylesheets="@../css/styles.css">
         <children>
            <Label layoutY="14.0" text="Update invoice">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="15.0" />
               </padding>
               <font>
                  <Font name="Berlin Sans FB" size="18.0" />
               </font>
            </Label>
            <Separator layoutX="14.0" layoutY="44.0" prefHeight="0.0" prefWidth="336.0" />
            <VBox layoutX="14.0" layoutY="64.0" prefHeight="78.0" prefWidth="182.0">
               <children>
                  <Label prefHeight="34.0" prefWidth="42.0" text="DATES" />
                  <DatePicker fx:id="datePickerDates" prefHeight="45.0" prefWidth="173.0" />
               </children>
            </VBox>
            <VBox layoutX="199.0" layoutY="64.0" prefHeight="80.0" prefWidth="151.0">
               <children>
                  <Label prefHeight="34.0" prefWidth="54.0" text="STAFF ID" />
                  <ComboBox fx:id="comboboxStaffID" prefHeight="45.0" prefWidth="150.0" />
               </children>
            </VBox>
            <VBox layoutX="14.0" layoutY="151.0" prefHeight="80.0" prefWidth="335.0">
               <children>
                  <Label prefHeight="34.0" prefWidth="42.0" text="STAFF" />
                  <TextField fx:id="textfiledStaff" prefHeight="46.0" prefWidth="366.0" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="CANCEL" />
      <ButtonType fx:constant="APPLY" />
   </buttonTypes>
</DialogPane>
